{
  "name": "toyDbApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android-build": "cd android && ./gradlew assembleWantongRelease",
    "android-install": "cd android && ./gradlew installWantongRelease",
    "configureJPush": "node node_modules/jpush-react-native/JPushConfiguration.js",
    "lint": "eslint --ext .js ./src --fix",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start:ios": "react-native run-ios",
    "start:5": "react-native run-ios --simulator=\"iPhone 5\"",
    "start:6sp": "react-native run-ios --simulator=\"iPhone 6s Plus\"",
    "start:7": "react-native run-ios --simulator=\"iPhone 7\"",
    "start:7p": "react-native run-ios --simulator=\"iPhone 7 Plus\"",
    "start:android": "react-native run-android --variant Wantongdebug",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "flow": "node_modules/.bin/flow",
    "flow-stop": "node_modules/.bin/flow stop"
  },
  "dependencies": {
    "ac-qrcode": "^1.0.1",
    "immutable": "^3.8.1",
    "jcore-react-native": "^1.1.3",
    "jpush-react-native": "^1.6.6",
    "react": "16.0.0-alpha.6",
    "react-native": "0.44.0",
    "react-native-animatable": "^1.2.0",
    "react-native-camera": "^0.8.0",
    "react-native-code-push": "^2.0.3-beta",
    "react-native-root-toast": "^1.1.2",
    "react-native-swiper": "^1.5.4",
    "react-navigation": "^1.0.0-beta.10",
    "react-redux": "^5.0.5",
    "redux": "^3.6.0",
    "redux-immutable": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^4.8.0",
    "redux-persist-immutable": "^4.3.0",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react-native": "^1.9.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "flow-bin": "^0.46.0",
    "jest": "^20.0.3",
    "pre-commit": "^1.2.2",
    "react-test-renderer": "16.0.0-alpha.6"
  },
  "jest": {
    "preset": "react-native"
  },
  "pre-commit": [
    "lint"
  ]
}
