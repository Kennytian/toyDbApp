{
  "name": "toyDbApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android-build": "cd android && ./gradlew assembleRelease",
    "android-install": "cd android && ./gradlew installRelease",
    "configureJPush": "node node_modules/jpush-react-native/JPushConfiguration.js",
    "lint": "eslint --ext .js ./src --fix",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start:ios": "react-native run-ios",
    "start:5": "react-native run-ios --simulator=\"iPhone 5\"",
    "start:6sp": "react-native run-ios --simulator=\"iPhone 6s Plus\"",
    "start:7": "react-native run-ios --simulator=\"iPhone 7\"",
    "start:7p": "react-native run-ios --simulator=\"iPhone 7 Plus\"",
    "start:android": "react-native run-android",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "flow": "node_modules/.bin/flow",
    "flow-stop": "node_modules/.bin/flow stop"
  },
  "dependencies": {
    "immutable": "^3.8.1",
    "jcore-react-native": "^1.1.0",
    "jpush-react-native": "^1.6.0",
    "react": "16.0.0-alpha.6",
    "react-native": "^0.43.4",
    "react-native-animatable": "^1.1.1",
    "react-native-root-toast": "^1.0.5",
    "react-navigation": "^1.0.0-beta.9",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-immutable": "^4.0.0",
    "redux-logger": "^3.0.1",
    "redux-persist": "^4.6.0",
    "redux-persist-immutable": "^4.2.0",
    "redux-promise": "^0.5.3",
    "redux-saga": "^0.14.8",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "19.0.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react-native": "1.9.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "flow-bin": "^0.44.2",
    "jest": "19.0.2",
    "pre-commit": "^1.2.2",
    "react-test-renderer": "16.0.0-alpha.6"
  },
  "jest": {
    "preset": "react-native"
  },
  "pre-commit": [
    "lint"
  ]
}
